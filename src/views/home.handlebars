<header>
  <div class="up_page">
    <a href="/home"><img
        class="logo"
        src="https://i.postimg.cc/JzVpc1S5/logo-header.png"
        alt="logo"
        width="80"
      /></a>
  </div>
  <nav class="navbar">
    <a href="/home">Home</a>
    <a href="/products">Products Data</a>
    <a href="/realtimeproducts">Update Products</a>
    <a href="/chat">Chat</a>
  </nav>

  <h1 class="titulo"> Horisada´s Paintings </h1>
  <h4 class="greeting">いらっしゃいませ</h4>

  <div class="div_space"></div>

</header>

<main>

  <h2 class="subtitulo"> Products Data </h2>

  <div class="table_container">
    {{#each products}}
      <div class="container_cards">
        <div class="card">
          <img src="{{thumbnail}}" class="card-img-top" alt="{{title}}" />
          <div class="card-body">
            <p>{{title}}</p>
            <p> {{description}} </p>
            <p>Us${{price}}</p>
            <button class="btn_cart">Add to cart </button>
          </div>
        </div>
      </div>
    {{/each}}

  </div>
</main>

<footer></footer>