    </div>
    <nav class="navbar2">
      <a href="/profile">Profile</a>
      <a onclick="logout()" style="cursor: pointer;">Logout</a>
    </nav>
  </div>
{{else}}
  <nav class="navbar2">
    <a href="/register">Register</a>
    <a href="/login">Login</a>
    <a href="/api/sessions/github">Login with Github</a>
  </nav>
{{/if}}